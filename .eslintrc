{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google"],

  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "spaced-comment": "off",
    "indent": "off",
    "require-jsdoc": "off",
    "max-len": "off"
  }
}